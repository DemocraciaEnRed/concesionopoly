*
  box-sizing: border-box

body,
html
  margin: 0
  padding: 0

body
  background-color: #E9E5DB

.concesionopoly
  z-index: 0
  position: relative
  padding: 10px 10px 10px 210px
  min-width: 600px
  min-height: 100vh
  color: #333
  font-family: 'Lato', sans-serif
  font-weight: 300
  background-color: #E9E5DB

  .sidebar
    position: absolute
    left: 10px
    top: 10px
    width: 190px

  @import 'concesionopoly/*'
